{
  "$schema": "https://json.schemastore.org/angular",
  "version": 1,
  "projects": {
    "prezentacja-angular": {
      "projectType": "application",
      "root": "",
      "sourceRoot": "src",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/prezentacja-angular",
            "index": "index.html",
            "main": "src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "tsconfig.json",
            "assets": ["src/assets"],
            "styles": ["src/styles.css"],
            "scripts": []
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "prezentacja-angular:build"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}